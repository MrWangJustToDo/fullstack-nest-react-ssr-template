{
  "name": "full-stack-ssr-template",
  "version": "1.0.0",
  "description": "",
  "author": "MrWangJustToDo",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "node dev.js",
    "build": "pnpm run build:packages && pnpm run build:app",
    "dev:packages": "ts-node ./scripts/rollupWatch.ts",
    "build:packages": "ts-node ./scripts/rollupBuild.ts",
    "dev:app": "cd app && pnpm run start:dev",
    "build:app": "cd app && pnpm run build",
    "build:static": "cd app && pnpm run build:static",
    "start:app": "cd app && pnpm run start:prod",
    "theme": "chakra-cli tokens ./ui/src/shared/theme.ts",
    "lint": "eslint --cache --ext ts,tsx .",
    "lint:fix": "yarn run lint --fix",
    "prettier": "prettier --ignore-path .prettierignore --write .",
    "clean": "pnpm -p -r -c exec rm -rf dist .cache dev",
    "purge": "pnpm -p -r -c exec rm -rf node_modules",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/runtime": "^7.20.6",
    "@chakra-ui/cli": "^2.2.0",
    "@chakra-ui/react": "^2.4.2",
    "@chakra-ui/styled-system": "^2.4.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@swc/core": "^1.3.21",
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "framer-motion": "^6.5.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "prettier": "^2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "^3.5.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tslib": "^2.4.1",
    "typescript": "^4.7.4"
  }
}
